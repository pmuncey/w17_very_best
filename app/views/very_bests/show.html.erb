<div class="page-header">
  <h3>Very Best #<%= @very_best.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Food</dt>
      <dd><%= @very_best.food_id %></dd>

      <dt>Venue</dt>
      <dd><%= @very_best.venue_id %></dd>

      <dt>User</dt>
      <dd><%= @very_best.user_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/very_bests" class="btn btn-primary">
        Back
      </a>
      <a href="/very_bests/<%= @very_best.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_very_best/<%= @very_best.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
